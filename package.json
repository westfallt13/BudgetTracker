{
  "name": "budget-tracker",
  "version": "1.0.0",
  "description": "Multi-account expense tracker application",
  "main": "main.js",
  "scripts": {
    "start": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "electron": "electron .",
    "electron-dev": "npm run build && electron .",
    "package-win": "npm run build && electron-packager . BudgetTracker --platform=win32 --arch=x64 --icon=Icons/electron-icon.ico --out=release --overwrite --asar"
  },
  "keywords": [
    "budget",
    "expense",
    "tracker",
    "finance"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.8.1",
    "electron": "^39.2.7",
    "electron-packager": "^17.1.2",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
